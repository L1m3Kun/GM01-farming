{
    "rewrites": [
      { "source": "/", "destination": "/index.html" },
      { "source": "/status", "destination": "/status.html" },
      { "source": "/ranking", "destination": "/ranking.html" }
    ],
    "builds": [
        { "src": "*.html", "use": "@vercel/static" },
        { "src": "style.css", "use": "@vercel/static" },
        { "src": "*.js", "use": "@vercel/static" }
      ]
  }
  